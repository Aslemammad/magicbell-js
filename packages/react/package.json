{
  "name": "@magicbell/magicbell-react",
  "version": "10.3.0",
  "description": "React components for building a notification inbox for your app",
  "author": "MagicBell <bot@magicbell.io> (https://magicbell.com/)",
  "contributors": [
    "Josue Montano <josuemontanoa@gmail.com>",
    "Stephan Meijer <stephan.meijer@gmail.com>",
    "Eric Hosick <erichosick@gmail.com>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "source": "./src/index.ts",
  "main": "dist/index.js",
  "module": "dist/magicbell-react.esm.js",
  "unpkg": "dist/magicbell-react.umd.production.min.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "/dist",
    "/src"
  ],
  "homepage": "https://magicbell.com",
  "keywords": [
    "bell",
    "magicbell",
    "notifications",
    "notification center",
    "notification inbox",
    "immer",
    "zustand",
    "immutable",
    "typescript",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:magicbell-io/magicbell-react.git"
  },
  "bugs": {
    "url": "https://github.com/magicbell-io/magicbell-react/issues"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && yarn build:dev && yarn build:prod",
    "build:dev": "vite build -c ../../scripts/vite/vite.config.js",
    "build:prod": "vite build -c ../../scripts/vite/vite.config.js --minify",
    "start": "yarn build:dev --watch",
    "size": "size-limit",
    "lint": "tsc --noEmit && eslint '{src,stories,tests}/**/*.{js,jsx,ts,tsx}'"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{md,html,css,scss}": "prettier --write"
  },
  "size-limit": [
    {
      "path": "dist/magicbell-react.cjs.production.min.js",
      "limit": "250 KB"
    },
    {
      "path": "dist/magicbell-react.umd.production.min.js",
      "limit": "250 KB"
    },
    {
      "path": "dist/magicbell-react.esm.js",
      "limit": "175 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/faker": "^5.5.9",
    "@types/lodash": "^4.14.184",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/rosie": "^0.0.40",
    "@types/sinon": "^10.0.6",
    "@types/tinycolor2": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-loader": "^8.2.2",
    "codecov": "^3.8.3",
    "dts-cli": "^1.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "lint-staged": "^12.1.3",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rosie": "^2.1.0",
    "sinon": "^13.0.1",
    "size-limit": "^8.0.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@magicbell/react-headless": "^4.2.0",
    "@tippyjs/react": "^4.2.5",
    "axios": "^0.27.2",
    "dayjs": "^1.11.5",
    "humps": "^2.0.1",
    "immer": "^9.0.7",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "miragejs": "^0.1.42",
    "ramda": "^0.28.0",
    "react-infinite-scroll-component": "^6.0.0",
    "react-use": "^17.3.2",
    "timeago.js": "^4.0.2",
    "tiny-invariant": "^1.2.0",
    "tiny-warning": "^1.0.3",
    "tinycolor2": "^1.4.2",
    "tslib": "^2.3.1",
    "zustand": "^3.6.4"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  }
}
